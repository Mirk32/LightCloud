<!DOCTYPE html>
<html>
<head>
  <title>LightCloud</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <%= yield %>
  </div>
  <div id="flash">
    <%=render ('shared/flash_alert')%>
  </div>
  <%= debug(params) if Rails.env.development? %>
  <%= javascript_include_tag "transform" %>
  <%= javascript_include_tag "knobKnob.jquery" %>
  <%= javascript_include_tag "jquery.cassette" %>
</body>
</html>
